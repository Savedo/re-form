{
  "name": "re-form",
  "version": "0.0.0-semantically-released",
  "description": "react form library",
  "author": "Savedo GmbH. <it@savedo.de> (https://www.savedo.de)",
  "license": "UNLICENSED",
  "homepage": "https://github.com/Savedo/re-form#readme",
  "main": "dist/re-form.js",
  "typings": "./index.d.ts",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Savedo/re-form.git"
  },
  "keywords": [
    "react",
    "form",
    "hooks"
  ],
  "files": [
    "src",
    "dist",
    "index.d.ts"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "prepare": "yarn clean && yarn lint && yarn test && yarn build:umd && yarn build:umd:min",
    "lint": "tslint -c tslint.json 'src/**/*.ts?(x)'",
    "test": "jest --passWithNoTests",
    "build": "yarn build:umd && yarn build:umd:min",
    "build:umd": "webpack --mode=development",
    "build:umd:min": "webpack --mode=production",
    "start": "yarn clean && yarn build:umd && webpack-dev-server --mode=development --progress --open"
  },
  "dependencies": {
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "yup": "^0.27.0"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0",
    "@types/react": "^16.8.13",
    "@types/react-dom": "^16.8.3",
    "@types/yup": "^0.26.12",
    "babel-loader": "^8.0.5",
    "html-webpack-plugin": "^3.2.0",
    "jest": "^24.7.1",
    "rimraf": "^2.6.3",
    "ts-loader": "^5.3.3",
    "tslint": "^5.15.0",
    "tslint-eslint-rules": "^5.4.0",
    "tslint-react": "^4.0.0",
    "typescript": "^3.4.3",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.3.1"
  },
  "peerDependencies": {
    "react": "*",
    "react-dom": "*"
  }
}
